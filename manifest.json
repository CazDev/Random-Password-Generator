{
    "manifest_version": 2,
    "name": "Random Password Generator",
    "version": "1.0.1",
    "description": "Generate a random password",
  
    "browser_action": {
      "default_icon": "./icons/icon-48.png"
    },

    "icons": {
		  "48": "./icons/icon-48.png",
		  "96": "./icons/icon-96.png",
		  "144": "./icons/icon-144.png"
	  },
	
    "background": {
      "persistent": true,
      "scripts": ["gen.js"]
    },

    "permissions": [
        "clipboardWrite"
    ]
  }