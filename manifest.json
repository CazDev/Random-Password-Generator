{
    "manifest_version": 2,
    "name": "Random Password Generator",
    "version": "1.0",
    "description": "Generate a random password",
    "browser_action": {
        "default_icon": "./icon.png"
    },
    "background": {
      "persistent": false,
      "scripts": ["gen.js"]
    },
    "permissions": [
        "tabs",
        "clipboardWrite"
    ]
  }